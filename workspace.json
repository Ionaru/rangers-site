{
    "version": 2,
    "projects": {
        "api": {
            "root": "apps/api",
            "sourceRoot": "apps/api/src",
            "projectType": "application",
            "prefix": "api",
            "targets": {
                "build-image": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "nx build api --production",
                                "forwardAllArgs": false
                            },
                            {
                                "command": "docker build -f ./apps/api/Dockerfile -t api:{args.tag} ."
                            }
                        ],
                        "parallel": false
                    }
                },
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "generatePackageJson": true,
                        "outputPath": "dist/apps/api",
                        "main": "apps/api/src/main.ts",
                        "tsConfig": "apps/api/tsconfig.app.json"
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "api:build"
                    }
                },
                "serve-all": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "nx serve api"
                            }
                        ],
                        "parallel": true
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/api/**/*.ts"
                        ]
                    },
                    "outputs": [
                        "{options.outputFile}"
                    ]
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": [
                        "coverage/apps/api"
                    ],
                    "options": {
                        "jestConfig": "apps/api/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "entities": {
            "root": "libs/entities",
            "sourceRoot": "libs/entities/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/entities/**/*.ts"
                        ]
                    },
                    "outputs": [
                        "{options.outputFile}"
                    ]
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": [
                        "coverage/libs/entities"
                    ],
                    "options": {
                        "jestConfig": "libs/entities/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "interfaces": {
            "root": "libs/interfaces",
            "sourceRoot": "libs/interfaces/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/interfaces/**/*.ts"
                        ]
                    },
                    "outputs": [
                        "{options.outputFile}"
                    ]
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": [
                        "coverage/libs/interfaces"
                    ],
                    "options": {
                        "jestConfig": "libs/interfaces/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "legacy": {
            "root": "apps/legacy",
            "sourceRoot": "apps/legacy/src",
            "projectType": "application",
            "prefix": "legacy",
            "targets": {
                "build-image": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            {
                                "command": "npx tsc -p apps/legacy/tsconfig.app.json",
                                "forwardAllArgs": false
                            },
                            {
                                "command": "nx prepare legacy",
                                "forwardAllArgs": false
                            },
                            {
                                "command": "nx build legacy --production",
                                "forwardAllArgs": false
                            },
                            {
                                "command": "docker build -f ./apps/legacy/Dockerfile -t legacy:{args.tag} ."
                            }
                        ],
                        "parallel": false
                    }
                },
                "prepare": {
                    "executor": "@nrwl/workspace:run-commands",
                    "options": {
                        "command": "npx node-sass --source-map true --output-style compressed --output apps/legacy/src/assets/stylesheets apps/legacy/src/app/styles"
                    }
                },
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "generatePackageJson": true,
                        "showCircularDependencies": false,
                        "outputPath": "dist/apps/legacy",
                        "main": "apps/legacy/src/main.ts",
                        "tsConfig": "apps/legacy/tsconfig.app.json",
                        "assets": [
                            "apps/legacy/src/assets",
                            "apps/legacy/src/app/views"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "legacy:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/legacy/**/*.ts"
                        ]
                    },
                    "outputs": [
                        "{options.outputFile}"
                    ]
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": [
                        "coverage/apps/legacy"
                    ],
                    "options": {
                        "jestConfig": "apps/legacy/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        }
    }
}
